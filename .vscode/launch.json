    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": [
                //fixed
                "--start", "0",
                "--end", "1000",
                "--length", "10",
                "--reference_model", "ViT-g-14",
                "--reference_model_pretrain", "laion2b_s12b_b42k",
                "--with_tracking",
                //watermark
                "--w_channel", "3",
                "--w_pattern", "zeros",
                // distortion
                // "--crop_ratio", "0.75",
                // "--crop_scale", "0.75",
                //experiment
                "--run_name", "zeros__ours, w ed_noise",
                "--solver_order", "2",
                "--num_inference_steps", "10",
                //"--exact_inversion",

                "--edcorrector",
                //"--inv_naive",

                "--inv_order", "2",
                "--test_num_inference_steps", "10",
                "--prompt_reuse",

            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
    ]
}